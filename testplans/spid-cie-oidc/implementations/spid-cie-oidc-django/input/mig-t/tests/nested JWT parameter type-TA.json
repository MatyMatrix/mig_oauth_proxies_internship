{
  "test suite": {
    "name": "TA",
    "description": "nested JWT parameter type",
    "filter messages": true
  },
  "tests": [
    {
      "test": {
        "name": "Does the Trust Mark contain a correct id_code claim",
        "description": "In this test, an issued Trust Mark must be taken, decrypted and the value of the id_code claim is a JSON Object",
        "type": "passive",
        "sessions": [
          "s1"
        ],
        "operations": [
          {
            "message type": "Entity Statement response OP",
            "decode operation": [
              {
                "from": "body",
                "type": "jwt",
                "decode param": "[\\s\\S]*",
                "decode operation": [
                  {
                    "from": "jwt payload",
                    "type": "jwt",
                    "decode param": "$.trust_marks[0].trust_mark",
                    "check": [
                      {
                        "in": "payload",
                        "check regex": "id_code.^\\{(\\s*\"[^\"]*\"\\s*:\\s*(?:\"[^\"]*\",?|\\[[\\S\\s]*\\],?|\\{[\\S\\s]*\\},?)\\s*)*\\}$"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    {
      "test": {
        "name": "Does the Trust Mark contain a correct sub claim",
        "description": "In this test, an issued Trust Mark must be taken, decrypted and the value of the sub claim has to be an URL",
        "type": "passive",
        "sessions": [
          "s1"
        ],
        "operations": [
          {
            "message type": "Entity Statement response OP",
            "decode operation": [
              {
                "from": "body",
                "type": "jwt",
                "decode param": "[\\s\\S]*",
                "decode operation": [
                  {
                    "from": "jwt payload",
                    "type": "jwt",
                    "decode param": "$.trust_marks[0].trust_mark",
                    "check": [
                      {
                        "in": "payload",
                        "check regex": "sub.https:\\/\\/(www\\.)?[-\\w@:%.\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-\\w()@:%\\+.~#?&\\/\\/=]*)"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    },
